{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "azdEnvName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "applicationName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "chatImageName": {
        "value": "${SERVICE_SIMPLE_CHAT_IMAGE_NAME}"
      },
      "chatAppExists": {
        "value": "${SERVICE_SIMPLE_CHAT_RESOURCE_EXISTS}"
      },
      "myIpAddress": {
        "value": "${MY_IP}"
      },
      "addRoleAssignments" : {
        "value": "${ADD_ROLE_ASSIGNMENTS=true}"
      },
      "publicAccessEnabled": {
        "value": "${PUBLIC_ACCESS_ENABLED=true}"
      },
      "searchServiceSkuName": {
        "value": "${AZURE_SEARCH_SERVICE_SKU=basic}"
      },
      "greenSoftwareSearchIndexName": {
        "value": "${AZURE_PATTERNS_SEARCH_INDEX=greensoftware-intvect}"
      },
      "computeDataSearchIndexName": {
        "value": "${AZURE_COMPUTE_SEARCH_INDEX=compute-intvect}"
      },
      "searchSemanticConfiguration":{
        "value": "${AZURE_SEARCH_SEMANTIC_CONFIGURATION=default}"
      },
      "searchIdentifierField": {
        "value": "${AZURE_SEARCH_IDENTIFIER_FIELD=chunk_id}"
      },
      "searchContentField": {
        "value": "${AZURE_SEARCH_CONTENT_FIELD=chunk}"
      },
      "searchTitleField": {
        "value": "${AZURE_SEARCH_TITLE_FIELD=title}"
      },
      "searchEmbeddingField": {
        "value": "${AZURE_SEARCH_EMBEDDING_FIELD=text_vector}"
      },
      "searchUseVectorQuery": {
        "value": "${AZURE_SEARCH_USE_VECTOR_QUERY=true}"
      },
      "searchServiceSemanticRankerLevel": {
        "value": "${AZURE_SEARCH_SEMANTIC_RANKER=free}"
      },
      "openAI_deploy_location": {
        "value": "${AZURE_OPENAI_DEPLOY_LOCATION}"
      },
      "addAppRegistration": {
        "value": "${ADD_APP_REGISTRATION=false}"
      },
      "addPrivateLinkForSearch": {
        "value": "${ADD_PRIVATE_LINK_FOR_SEARCH=true}"
      },
      "addFirewallRuleForMyIp": {
        "value": "${ADD_FIREWALL_RULE_FOR_MY_IP=true}"
      }
    }
  }